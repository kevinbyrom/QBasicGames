DO
REM RUSSELL SEBRING
SCREEN 13
REM DOTS
FOR COUNT = 1 TO 3000
X = INT(320 * RND)
Y = INT(200 * RND)
PSET (X, Y), INT(256 * RND)
NEXT COUNT
LINE (10, 5)-(310, 195), 10, BF
REM CIRCLES
DO
CIR = CIR + 1
 X = INT(320 * RND)
 Y = INT(320 * RND)
CIRCUM = INT(50 * RND)
SHADE = INT(50 * RND)
CIRCLE (X, Y), CIRCUM, SHADE
IF CIR > 250 THEN PAINT (X, Y), SHADE, SHADE
LOOP UNTIL CIR = 500
REM LOONEY TUNES
CIR = 121
DO
SHADE = INT(256 * RND)
CIR = CIR - 1
CIRCLE (160, 100), CIR, SHADE
PAINT (160, 100), SHADE, SHADE
LOOP UNTIL CIR = 0
REM BOXES
FOR COUNT = 1 TO 2
X2 = 320
Y2 = 200
X1 = -1
Y1 = -1
DO
X1 = X1 + 1
X2 = X2 - 1
Y1 = Y1 + 1
Y2 = Y2 - 1
SHADE = INT(256 * RND)
LINE (X1, Y1)-(X2, Y2), SHADE, B
LOOP UNTIL Y2 = 0
NEXT
REM the end!
PRINT "RUSSELL'S PROGRAM";
LOOP

