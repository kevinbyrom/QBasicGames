IF MX(3) > X THEN MX(3) = MX(3) - 1
IF MY(1) < Y THEN MY(1) = MY(1) + 1
IF MY(2) < Y THEN MY(2) = MY(2) + 1
IF MY(3) < Y THEN MY(3) = MY(3) + 1
IF MY(1) > Y THEN MY(1) = MY(1) - 1
IF MY(2) > Y THEN MY(2) = MY(2) - 1
IF MY(3) > Y THEN MY(3) = MY(3) - 1
LOCATE MY(1), MX(1)
COLOR 3
PRINT CHR$(1)
LOCATE MY(2), MX(2)
COLOR 6
PRINT CHR$(1)
LOCATE MY(3), MX(3)
COLOR 5
PRINT CHR$(1)
COLOR 15
IF MX(1) = X AND MY(1) = Y THEN EXIT DO
IF MX(2) = X AND MY(2) = Y THEN EXIT DO
IF MX(3) = X AND MY(3) = Y THEN EXIT DO
LOOP
IF L$ = "YES" THEN
SOUND 80, 3
SOUND 300, 2
L$ = "NO"
X = INT(RND * 80) + 1
Y = INT(RND * 20) + 1
GOTO SKIP
END IF
CLS
PRINT "GAME OVER!!"
PRINT "SCORE "; (S * 100)
INPUT "PLAY AGAIN ( Y OR N )> ", ASK$
IF ASK$ = "Y" OR ASK$ = "y" THEN
IF (S * 100) > HI THEN HI = (S * 100)
CLS
GOTO RESTART
END IF
